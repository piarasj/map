{
  "name": "MapaLister - Interactive Data Mapping",
  "short_name": "MapaLister",
  "description": "Transform your data into beautiful, interactive maps with powerful listing and search capabilities",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#10b981",
  "background_color": "#ffffff",
  "lang": "en-US",
  "categories": ["productivity", "utilities", "navigation"],
  
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "72x72",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "96x96",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "144x144",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  
  "shortcuts": [
    {
      "name": "Search Contacts",
      "short_name": "Search",
      "description": "Search through contact listings",
      "url": "/?action=search"
    },
    {
      "name": "Zoom to All",
      "short_name": "Zoom All", 
      "description": "Zoom map to show all contacts",
      "url": "/?action=zoom-all"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "geojson_file",
          "accept": [".geojson", ".json", "application/geo+json"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/open-geojson",
      "accept": {
        "application/geo+json": [".geojson"],
        "application/json": [".json"]
      }
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "geo",
      "url": "/?lat=%s&lng=%s"
    }
  ],
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}